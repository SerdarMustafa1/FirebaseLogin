{
  "expo": {
    "name": "Nested Navigation",
    "slug": "nestednavi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.popeating.myapp",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses locations to find restaurants near you.",
        "CFBundleAllowMixedLocalizations": true
      }
    },
    "android": {
      "package": "com.popeating.myapp",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Basic Nested navigation with drawer and tabs, dark mode, and maps",
    "githubUrl": "https://github.com/popeating/myApp",
    "extra": {

    }
  }
}